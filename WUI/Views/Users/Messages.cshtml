@model IEnumerable<BLL.DTO.UserDTO>

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<div id="messageContent">
    
</div>
<script>
fetch(`/Users/Messages`)
                .then((response) => {
                    return response.text();
                })
                .then((result) => {
                    document.getElementById('messageContent').innerHTML = result;
                });
</script>
